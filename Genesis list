noButton.Size = isMobile and UDim2.new(0, 100, 0, 32) or UDim2.new(0, 120, 0, 36)
noButton.Position = isMobile and UDim2.new(1, -120, 1, -42) or UDim2.new(1, -140, 1, -46)
